#!"/home/user/python learning/hexlet/python-project-lvl1/.venv/bin/python"
# EASY-INSTALL-ENTRY-SCRIPT: 'formindev-brain-games','console_scripts','brain-games'
__requires__ = 'formindev-brain-games'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('formindev-brain-games', 'console_scripts', 'brain-games')()
    )
